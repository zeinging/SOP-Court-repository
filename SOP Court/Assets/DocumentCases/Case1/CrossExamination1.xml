<Case>
  <CrossExamination onStand="Sahwit">
    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>I was going door-to-door</Line>
        <Line>selling subscriptions when I saw</Line>
        <Line>a man fleeing an apartment.</Line>
      </TestimonyParagraph>
      <PressedInteractions>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Isn't a man leaving an</Line>
            <Line>apartment a common sight?</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>I find it odd you would</Line>
            <Line>take notice of him...</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Er... heh.</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>I don't know. He just seemed</Line>
            <Line>strange to me, that's all</Line>
          </Paragraph>
        </PressedInteraction>


        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Like he was mad, and yet</Line>
            <Line>frightended at the same time.</Line>
          </Paragraph>
        </PressedInteraction>


        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Just like... a criminal fleeing</Line>
            <Line>the scene of a crime!</Line>
          </Paragraph>
        </PressedInteraction>


        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>The defence requests that</Line>
            <Line>the witness refrain from</Line>
            <Line>conjecture!</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>Of course. What the witness</Line>
            <Line>means is that the man he saw</Line>
            <Line>looked suspicious!</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>So, what happened next?</Line>
          </Paragraph>
        </PressedInteraction>
      </PressedInteractions>
    </TestimonySeries>

    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>I thought he must be in a</Line>
        <Line>hurry because he left the</Line>
        <Line>door half-open behind him.</Line>
      </TestimonyParagraph>
      <PressedInteractions>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Half-open... you say?</Line>
            <Line>apartment a common sight?</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Yes, yes, the door was</Line>
            <Line>open halfway. Yes.</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>I watched for a moment, but</Line>
            <Line>no one came to close the door.</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>"Thats odd, in a big city</Line>
            <Line>like this", I thought...</Line>
          </Paragraph>
        </PressedInteraction>


        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>I see.</Line>
            <Line>And what happened next?</Line>
          </Paragraph>
        </PressedInteraction>
      </PressedInteractions>
    </TestimonySeries>
    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>Thinking it strange, I looked</Line>
        <Line>inside the apartment.</Line>
      </TestimonyParagraph>
      <PressedInteractions>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>What gave you the </Line>
            <Line>idea to do that?</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Well the door was half-open,</Line>
            <Line>you see.</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Isn't it only human</Line>
            <Line>to want to... peek?</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>We climb mountains because</Line>
            <Line>they are there!</Line>
            <Line>It's the same thing.</Line>
          </Paragraph>
        </PressedInteraction>


        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>Truer words have never</Line>
            <Line>been spoken!</Line>
            <Line>Anyone would have looked inside!</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>(Hmm... why did Payne cut</Line>
            <Line>him off so quickly?)</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>So you looked into the</Line>
            <Line>apartment.</Line>
            <Line>What happened then?</Line>
          </Paragraph>
        </PressedInteraction>

      </PressedInteractions>
    </TestimonySeries>

    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>Then I saw here lying there...</Line>
        <Line>A woman... not moving... dead!</Line>
      </TestimonyParagraph>
      <PressedInteractions>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Are you sure she was dead?</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>W-Well, no, I guess I wasn't.</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>But, she wasn't moving at</Line>
            <Line>all, and there was blood</Line>
            <Line>everywhere.</Line>
          </Paragraph>
        </PressedInteraction>

        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>(I guess that would look </Line>
            <Line>fatal to anyone...)</Line>
          </Paragraph>
        </PressedInteraction>


        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Very well, what happened next?</Line>
          </Paragraph>
        </PressedInteraction>
      </PressedInteractions>
    </TestimonySeries>

    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>I quailed in a fright and found</Line>
        <Line>myself unable to go inside.</Line>
      </TestimonyParagraph>
      <PressedInteractions>

        <!-- The guide I was wasn't didn't press this part so I'm just doing a simple one for now -->
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Ok, I believe you!</Line>
          </Paragraph>
        </PressedInteraction>

      </PressedInteractions>
    </TestimonySeries>
    
    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>I thought to call the police</Line>
        <Line>immediately!</Line>
      </TestimonyParagraph>
      <PressedInteractions>

        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>You "thought" to call</Line>
            <Line>the police? Does that mean you</Line>
            <Line>didn't actually call them!?</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>Please, please... Listen to</Line>
            <Line>the rest of the testimony.</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>You thought to call the</Line>
            <Line>police... What happened next?</Line>
          </Paragraph>
        </PressedInteraction>

      </PressedInteractions>
    </TestimonySeries>
    
    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>However, the phone in her</Line>
        <Line>apartment wasn't working.</Line>
      </TestimonyParagraph>
      <PressedInteractions>

        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>The phone in her apartment</Line>
            <Line>wasn't working?</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Yes. I mean, no.</Line>
            <Line>No, it wasn't. Right.</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>But you said you didn't</Line>
            <Line>go into the apartment...</Line>
            <Line>or did you!?</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Oh, oh, that? I can explain</Line>
            <Line>that!</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>There was a coordless phone</Line>
            <Line>on a shelf in the entranceway.</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>I reached inside and tried</Line>
            <Line>using that to call...</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>And that phone wasn't</Line>
            <Line>working, correct?</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>What happened next?</Line>
          </Paragraph>
        </PressedInteraction>

      </PressedInteractions>
    </TestimonySeries>
    
    <TestimonySeries isConflicting="false">
      <TestimonyParagraph>
        <Line>I went to a nearby park and</Line>
        <Line>found a public phone.</Line>
      </TestimonyParagraph>
      <PressedInteractions>

        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Why use a public phone?</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Well, you see, I don't</Line>
            <Line>have a cell phone.</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>And, being the middle of the</Line>
            <Line>afternoon, there was no answer</Line>
            <Line>at the nearby apartments.</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Ah, right... what time</Line>
            <Line>did you call again?</Line>
          </Paragraph>
        </PressedInteraction>
      </PressedInteractions>
    </TestimonySeries>
    
    <TestimonySeries isConflicting="true">
      <TestimonyParagraph>
        <Line>I remember the time exactly:</Line>
        <Line>It was 1:00 PM.</Line>
      </TestimonyParagraph>
      <PressedInteractions>

        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
			  <Line>Are you sure?</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Nope. But you can't prove it otherwise</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
			  <Line>Watch me!</Line>
          </Paragraph>
        </PressedInteraction>
      </PressedInteractions>
      <PressedInteractions item="autopsy Report">

        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>You found the body at 1:00PM.</Line>
            <Line>You're sure?.</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Yes.</Line>
            <Line>It was 1:00 PM, for certain.</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Frankly, I find that hard to </Line>
            <Line>believe!</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>Your statement directly</Line>
            <Line>conflicts the autopsy report.</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>The autopsy notes the time</Line>
            <Line>of death at sometime after</Line>
            <Line>4PM.</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>There was nobody to... er...</Line>
            <Line>no "body" to find at 1:00 PM!</Line>
            <Line>4PM.</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Phoenix Wright</Character>
          <Paragraph>
            <Line>How do you explain this</Line>
            <Line>three hour gap?</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>!!!</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Oh, that! Oh, er...</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Payne</Character>
          <Paragraph>
            <Line>This is trivial! The witness</Line>
            <Line>merely forgot the time!</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Judge</Character>
          <Paragraph>
            <Line>After his testimony, I find</Line>
            <Line>that hard to believe.</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Judge</Character>
          <Paragraph>
            <Line>Mr. Sahwit...</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Judge</Character>
          <Paragraph>
            <Line>Why were you so certain</Line>
            <Line>that you found the body</Line>
            <Line>at 1:00 PM?</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>I... er... well, I...</Line>
            <Line>Gee, that's a really good</Line>
            <Line>question!</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Mia</Character>
          <Paragraph>
            <Line>Great job, Wright!</Line>
            <Line>Way to put him on the spot!</Line>
          </Paragraph>
        </PressedInteraction>
        
        <PressedInteraction>
          <Character>Mia</Character>
          <Paragraph>
            <Line>That's all you have to do:</Line>
            <Line>point out contradictions!</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Mia</Character>
          <Paragraph>
            <Line>Lies always beget more lies!</Line>
          </Paragraph>
        </PressedInteraction>
      
        <PressedInteraction>
          <Character>Mia</Character>
          <Paragraph>
            <Line>See through one, and their</Line>
            <Line>whole story falls apart!</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Sahwit</Character>
          <Paragraph>
            <Line>Wait!</Line>
            <Line>I remember now!</Line>
          </Paragraph>
        </PressedInteraction>
        <PressedInteraction>
          <Character>Judge</Character>
          <Paragraph>
            <Line>Would you care to give</Line>
            <Line>your testimony again?</Line>
          </Paragraph>
        </PressedInteraction>
      </PressedInteractions>
    </TestimonySeries>
  </CrossExamination>
</Case>
