<Case>
	<CrossExamination onStand="Craig">
		<TestimonySeries isConflicting="false">
			<TestimonyParagraph>
				<Line>I was hanging out with</Line>
				<Line>Circuit Man in my backyard</Line>
			</TestimonyParagraph>
			<PressedInteractions>
				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>Around what time were</Line>
						<Line>you with him?</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>I'd say it was around 2 in the afternoon.</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Prosecution</Character>
					<Paragraph>
						<Line>That would line up with</Line>
						<Line>the vicitim's time of</Line>
						<Line>death</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>Hm, true. Continue the</Line>
						<Line>testimony.</Line>
					</Paragraph>
				</PressedInteraction>
			</PressedInteractions>
		</TestimonySeries>

		<TestimonySeries isConflicting="false">
			<TestimonyParagraph>
				<Line>I had recently installed</Line>
				<Line>a pool and the two of us</Line>
				<Line>were breaking it in.</Line>
			</TestimonyParagraph>
			<PressedInteractions>
				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>Was anyone else suppose to be</Line>
						<Line>there that day?</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>Not that day, no. It was only us.</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>Though I was planning a bigger</Line>
						<Line>get-together that weekend.</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>Although this whole mess has kind of</Line>
						<Line>turned me off from pool parties...</Line>
					</Paragraph>
				</PressedInteraction>

			</PressedInteractions>
		</TestimonySeries>
		<TestimonySeries isConflicting="false">
			<TestimonyParagraph>
				<Line>At one point, I realized I </Line>
				<Line>had run out of soda and left</Line>
				<Line>to get more.</Line>
			</TestimonyParagraph>
			<PressedInteractions>
				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>What time did you leave</Line>
						<Line>the house?</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>2:20 PM, I'm pretty sure.</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>And I was only gone for</Line>
						<Line>about ten minutes.</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>*thinking* Plenty of time</Line>
						<Line>for someone to kill</Line>
						<Line>Circuit Man..</Line>
					</Paragraph>
				</PressedInteraction>
			</PressedInteractions>
		</TestimonySeries>

		<TestimonySeries isConflicting="false">
			<TestimonyParagraph>
				<Line>I drove out to the local</Line>
				<Line>supermarket and got a few</Line>
				<Line>cases.</Line>
			</TestimonyParagraph>
			<PressedInteractions>
				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>Is there anyone there who could</Line>
						<Line>prove you were there?</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>Uhh, no one I know. The cashier, maybe?</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Prosecution</Character>
					<Paragraph>
						<Line>Would the cashier even</Line>
						<Line>remember the defendant,</Line>
						<Line>though?</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Prosecution</Character>
					<Paragraph>
						<Line>The cashier probably sees a</Line>
						<Line>few hundred people a day.</Line>
					</Paragraph>
				</PressedInteraction>
				
				<PressedInteraction>
					<Character>Prosecution</Character>
					<Paragraph>
						<Line>Asking them to single out one</Line>
						<Line>person they saw would be</Line>
						<Line>rediculous.</Line>
					</Paragraph>
				</PressedInteraction>
			</PressedInteractions>
		</TestimonySeries>

		<TestimonySeries isConflicting="false">
			<TestimonyParagraph>
				<Line>When I got back, I saw Circuit</Line>
				<Line>Man at the bottom of the pool.</Line>
			</TestimonyParagraph>
			<PressedInteractions>

				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>Did he seem unconscious?</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>Well, he wasn't swimming around,</Line>
						<Line>that's for sure.</Line>
					</Paragraph>
				</PressedInteraction>

			</PressedInteractions>
		</TestimonySeries>

		<TestimonySeries isConflicting="false">
			<TestimonyParagraph>
				<Line>I managed to pull him out</Line>
				<Line>of the pool but he was already</Line>
				<Line>dead.</Line>
			</TestimonyParagraph>
			<PressedInteractions>

				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>How did you know he was dead?</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>He didn't have a pulse when I checked him.</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>What about the body? Did you</Line>
						<Line>notice anything about it?</Line>
					</Paragraph>
				</PressedInteraction>
				
				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>Like any sort of injuries?</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>I was too panicked to notice</Line>
						<Line>anything else.</Line>
					</Paragraph>
				</PressedInteraction>
			</PressedInteractions>
		</TestimonySeries>

		<TestimonySeries isConflicting="false">
			<TestimonyParagraph>
				<Line>Before I could call for help,</Line>
				<Line>I heard sirens coming down the</Line>
				<Line>street.</Line>
			</TestimonyParagraph>
			<PressedInteractions>

				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>So, you're saying someone else</Line>
						<Line>called the police?</Line>
					</Paragraph>
				</PressedInteraction>
				<PressedInteraction>
					<Character>Defence</Character>
					<Paragraph>
						<Line>None of the neighbors saw</Line>
						<Line>what happened though?</Line>
					</Paragraph>
				</PressedInteraction>

				<PressedInteraction>
					<Character>Craig</Character>
					<Paragraph>
						<Line>I have no idea.</Line>
					</Paragraph>
				</PressedInteraction>
			</PressedInteractions>
		</TestimonySeries>
	</CrossExamination>
</Case>
